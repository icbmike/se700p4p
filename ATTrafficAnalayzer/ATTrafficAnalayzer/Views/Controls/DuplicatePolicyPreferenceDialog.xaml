<Window x:Class="ATTrafficAnalayzer.Views.Controls.DuplicatePolicyPreferenceDialog"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Select a Default Duplicate Policy" Width="368" Height="146">
    <StackPanel>
        <Label Content="Select your preference for importing duplicate data" Margin="30, 0, 0, 0"/>
        <RadioButton GroupName="Preference" Content="Skip" Margin="30, 0, 0, 0"/>
        <RadioButton GroupName="Preference" Content="Continue" Margin="30, 0, 0, 0"/>
        <RadioButton GroupName="Preference" Content="Ask me every time" IsChecked="True" Margin="30, 0, 0, 0"/>
        <Button Content="Ok" Width="200" Margin="0, 10, 0, 0"/>
    </StackPanel>
</Window>
